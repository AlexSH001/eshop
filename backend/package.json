{
  "name": "eshop-backend",
  "version": "1.0.0",
  "description": "Backend API for eshop",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "echo 'No build step required'",
    "test": "jest",
    "migrate": "node src/database/migrate.js",
    "migrate-postgres": "node src/database/migrate-postgres.js",
    "migrate-full": "node src/database/migrate-full.js",
    "test-postgres": "node test-postgres-migration.js",
    "test-performance": "node test-performance-optimization.js",
    "test-monitoring": "node test-monitoring-simple.js",
    "seed": "node src/database/seed-universal.js",
    "seed-images": "node src/database/seed-with-images.js",
    "sync-data": "npm run seed",
    "logs:view": "tail -f logs/combined-$(date +%Y-%m-%d).log",
    "logs:error": "tail -f logs/error-$(date +%Y-%m-%d).log",
    "health:check": "curl -s https://10.170.0.4/api/monitoring/health | jq",
    "metrics:view": "curl -s https://10.170.0.4/api/monitoring/metrics"
  },
  "dependencies": {
    "@sentry/node": "^9.38.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "pg-pool": "^3.10.1",
    "prom-client": "^15.1.3",
    "redis": "^5.6.0",
    "sharp": "^0.34.3",
    "sqlite3": "^5.1.6",
    "stripe": "^18.5.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
